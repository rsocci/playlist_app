<h1>Create New Playlist</h1>

<%= form_for @playlist do |f| %>
	<% if @playlist.errors.any? %>
		<h2>Errors:</h2>
		<ul>
			<% @playlist.errors.full_messages.each do |msg| %>
			<li><%= msg %></li>
			<% end %>
		</ul>
	<% end %>

	<p>
		<%= f.label :link %><br>
		<%= f.text_field :link %>
	</p>
	<p><%= f.submit "Create" %></p>

<% end %>

<%= link_to "Back", playlists_path %>


