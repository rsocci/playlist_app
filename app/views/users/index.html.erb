<h1>List of Users</h1>
<% @users.each do |user| %>
	<h2> <%= link_to user.full_name, user %>
	</h2>
	<h3>Email: <%= user.email %></h3>
	<h3>Password: <%= user.password %></h3>
	<p><%= link_to "Edit", edit_user_path(user) %>
		<%= link_to "Delete", user, method: :delete, data: { confirm: 'Are you sure?'} %></p>
	<hr>
<% end %>

<p><%= link_to "New User", new_user_path %></p>